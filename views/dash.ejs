<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="dashStyle.css" type="text/css">
    <title>dashboard</title>
</head>

<body>
    <div class="container">
        <div>
            <div id="top-bar">
                <label>Welcome! <label id="greet"></label></label>
                <div id="navlist">
                    <div id="items">Home</div>
                    <div id="items">About</div>
                    <div id="items">Help</div>
                </div>
                <img src="img/logout.png" id="logout"></button>
            </div>
        </div>
        <div>
            <label>Current job: </label>
            <label>None</label>
        </div>
        <div class="display">
            <h2>Open Job applications</h2>
            <div>
                <div class="job-list horizontal-scroll">
                    <div id="ojs" class="job-card">
                        <div>Nem</div>
                        <div>company</div>
                        <div>id</div>
                    </div>
                </div>
            </div>
        </div>
        <div class="display">
            <h2>Closed Job applications</h2>
            <div>
                <div class="job-list horizontal-scroll">
                    <div id="cjs" class="job-card">
                        <div>Nem</div>
                        <div>company</div>
                        <div>id</div>
                    </div>
                </div>
            </div>
        </div>
        <div>
            <h2>Previous Jobs Job applications</h2>
            <div>
                <div class="job-list horizontal-scroll">
                    <div id="pjs" class="job-card">
                        <div>Nem</div>
                        <div>company</div>
                        <div>id</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div>Create new job
        <button>+</button>
    </div>
    <script src="dashboard.js" type="text/javascript"></script>
</body>

</html>